@model Data.VO.FiscalVO

<head>
    <link rel="stylesheet" href="~/css/Fiscal/edit.css">
</head>

<h2>Editar</h2>

<form asp-action="EditFiscalConfirmed" method="post">
    <div class="form-group">
        <label asp-for="Id" class="control-label"></label>
        <input asp-for="Id" class="form-control" readonly />
    </div>

    <div class="form-group">
        <label asp-for="Nome" class="control-label"></label>
        <input asp-for="Nome" class="form-control" required />
    </div>

    <div class="form-group">
        <label asp-for="Email" class="control-label"></label>
        <input asp-for="Email" class="form-control" required />
    </div>

    <br />

    <div class="form-group">
        <input type="submit" value="Editar" class="btn btn-primary" />
        <a asp-action="IndexFiscal" class="link-back">Voltar</a>
    </div>
</form>
