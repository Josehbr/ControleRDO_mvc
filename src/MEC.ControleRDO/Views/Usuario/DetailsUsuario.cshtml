@model Data.VO.UsuarioVO

<head>
    <link rel="stylesheet" href="~/css/Usuario/details.css">
</head>

<h2>Detalhes</h2>

<dl class="row">
    <dt class="col-sm-2">Id</dt>
    <dd class="col-sm-10">@Model.Id</dd>
</dl>
<dl class="row">
    <dt class="col-sm-2">Nome</dt>
    <dd class="col-sm-10">@Model.Nome</dd>
</dl>
<dl class="row">
    <dt class="col-sm-2">Email</dt>
    <dd class="col-sm-10">@Model.Email</dd>
</dl>
<dl class="row">
    <dt class="col-sm-2">Login</dt>
    <dd class="col-sm-10">@Model.Login</dd>
</dl>

<dl class="row">
    <dt class="col-sm-2">Perfil</dt>
    <dd class="col-sm-10">@Model.Perfil</dd>
</dl>


<form>
    <a asp-action="IndexUsuario" class="link-back">Voltar</a>
</form>
