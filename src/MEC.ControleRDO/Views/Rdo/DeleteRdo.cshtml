@model Data.VO.RdoVO

<h2>Delete Fiscal</h2>

<div>
    <h3>Are you sure you want to delete this?</h3>
    <dl class="row">
        <dt class="col-sm-2">Id</dt>
        <dd class="col-sm-10">@Model.Id</dd>
    </dl>
    <dl class="row">
        <dt class="col-sm-2">DataRdo</dt>
        <dd class="col-sm-10">@Model.DataRdo</dd>
    </dl>
    <dl class="row">
        <dt class="col-sm-2">NumeroOrcamento</dt>
        <dd class="col-sm-10">@Model.NumeroOrcamento</dd>
    </dl>
    <dl class="row">
        <dt class="col-sm-2">DataAssinatura</dt>
        <dd class="col-sm-10">@Model.DataAssinatura</dd>
    </dl>
    <dl class="row">
        <dt class="col-sm-2">Observacao</dt>
        <dd class="col-sm-10">@Model.Observacao</dd>
    </dl>
    <form asp-action="DeleteRdoConfirmed" method="post">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-danger" />
        <a asp-action="IndexRdo">Back to List</a>
    </form>
</div>
